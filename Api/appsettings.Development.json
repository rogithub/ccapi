{
  "Serilog": {
    "Using": [
      "Serilog.Sinks.RollingFile"
    ],
    "MinimumLevel": "Verbose",
    "WriteTo": [
      {
        "Name": "RollingFile",
        "Args": {
          "pathFormat": "logs/ccapi-dev-{Date}.txt"
        }
      }
    ],
    "Properties": {
      "Application": "ccapi"
    }
  }
}